.u-bg-grey-light.u-mb-5
  .container.u-py-5
    %h2 Popular tags right now
    -if @tags
      =render partial: "tags/tag", collection: @tags
    -else
      %p No tags found.

.container
  -unless @lists.empty?
    %h2 Popular lists right now
    =render @lists
  -else
    %p No lists created yet.
